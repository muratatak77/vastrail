<table class="table table-hover table-bordered">
  <thead>
    <tr>
      <th>No</th>
      <th>Title</th>
      <th>Video Url</th>
      <th>Clickthrough Url</th>
      <th>Created</th>
      <th>User</th>
      <th>Category</th>
      <th>Process</th>
    </tr>
  </thead>
  <tbody>
  	  <% creatives.forEach(function(c){ %>
        <tr>
          <td><%= c.id %></td>
          <td><%= c.title %></td>
          <td><%= c.video_url %></td>
          <td><%= c.clickthrough_url %></td>
          <td><%= c.createdDate %></td>
          <td>
            <% if (typeof c.user != "undefined" && c.user != null ){ %>
            <a href="/user/profile/<%= c.user.id %>" ><%= c.user.local.email %></a>
            <% } %>
          </td>

          <td>
            <% if (typeof c.category != "undefined" && c.category != null ){ %>
            <a href="/categories/show/<%= c.category.id %>" ><%= c.category.title %></a>
            <% } %>
          </td>

          <td>
            <a href="/creatives/show/<%= c.id %>">Show</a>
          	-
			       <a data-confirm="Are you sure?" data-method="DELETE" href="/creatives/destroy/<%= c.id %>" rel="nofollow">Destroy</a>
          	-
          	<a href="/creatives/edit/<%= c.id %>" >Edit</a> 
          </td>
        </tr>

     <% }); %>
  </tbody>
</table>