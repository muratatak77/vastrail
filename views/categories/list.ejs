<table class="table table-hover table-bordered">
  <thead>
    <tr>
      <th>No</th>
      <th>Title</th>
      <th>Name</th>
      <th>Process</th>
    </tr>
  </thead>
  <tbody>
  	  <% categories.forEach(function(c){ %>
        <tr>
          <td><%= c.id %></td>
          <td><%= c.title %></td>
          <td><%= c.cat_name %></td>
          <td>
            <a href="/categories/show/<%= c.id %>">Show</a>
          	-
			       <a data-confirm="Are you sure?" data-method="DELETE" href="/categories/destroy/<%= c.id %>" rel="nofollow">Destroy</a>
          	-
          	<a href="/categories/edit/<%= c.id %>" >Edit</a> 
          </td>
        </tr>

     <% }); %>
  </tbody>
</table>