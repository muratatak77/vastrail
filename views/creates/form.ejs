	<form class="form-horizontal" action="<%= form_action_page %>" method="<%= form_method_type %>">

	  <div class="form-group">
	    <label  class="col-sm-2 control-label">Title</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" value="<%= create.title %>" name="title" id="title" placeholder="Title">
	    </div>
	  </div>

	  <div class="form-group">
	    <label class="col-sm-2 control-label">Type</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" value="<%= create.type %>" name="type" id="type" placeholder="Type">
	    </div>
	  </div>

	  <div class="form-group">
	    <label class="col-sm-2 control-label">Advertisers</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" value="<%= create.advertisers %>"  name="advertisers" id="advertisers" placeholder="Advertisers">
	    </div>
	  </div>

	  <div class="form-group">
	    <label class="col-sm-2 control-label">Video Click URL</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" value="<%= create.video_clickthrough_url %>" name="video_clickthrough_url" id="video_clickthrough_url" placeholder="Video Click URL">
	    </div>
	  </div>


	  <div class="form-group">
	    <label class="col-sm-2 control-label">Skip</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" value="<%= create.skip %>" name="skip" id="skip" placeholder="Skip">
	    </div>
	  </div>

	  <div class="form-group">
	    <div class="col-sm-offset-2 col-sm-10">
	      <button type="submit" class="btn btn-default ">Submit</button>
	      <button type="reset" class="btn btn-default ">Reset</button>
	      <div id="cancel_btn" class="btn btn-default">Cancel</div>
	    </div>
	  </div>


	</form>

